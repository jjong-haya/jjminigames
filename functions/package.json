    "devDependencies": {
      "@types/node": "^20.11.0",
      "@typescript-eslint/eslint-plugin": "^7.18.0",
      "@typescript-eslint/parser": "^7.18.0",
      "eslint": "^8.57.0",
      "rimraf": "^5.0.0",
      "typescript": "^5.4.0"
    }
  }
